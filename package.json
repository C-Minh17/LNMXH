{
  "name": "slink-ais",
  "version": "5.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "dev": "max dev",
    "build": "max build",
    "format": "prettier --cache --write .",
    "postinstall": "max setup node ./postinstall.js",
    "setup": "max setup",
    "start": "npm run dev",
    "push-rsync": "rsync -avzhe ssh --delete-before dist/ ubuntu@203.162.10.113:/var/www/slink.ptit.edu.vn/",
    "push-rsync-win": "rsync -avzh --chmod=755 --no-perms --delete-before dist/ ubuntu@203.162.10.113:/var/www/slink.ptit.edu.vn/",
    "deploy": "yarn build && yarn push-rsync",
    "deploy-win": "yarn build && yarn push-rsync-win",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "max setup && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org aisoft --project sentry-vwa build && sentry-cli --url https://sentry.aisoftech.vn/ sourcemaps upload --org aisoft --project sentry-vwa build"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@ant-design/pro-components": "^2.8.6",
    "@ant-design/pro-layout": "7.22.3",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@monaco-editor/react": "^4.7.0",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "2.4.2",
    "@sentry/react": "^9.3.0",
    "@tinymce/tinymce-react": "^6.0.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react-highlight-words": "^0.20.0",
    "@types/styled-components": "^5.1.34",
    "@umijs/max": "^4.4.6",
    "@umijs/plugin-model": "^2.6.2",
    "@umijs/route-utils": "^4.0.1",
    "antd": "^5.24.2",
    "apexcharts": "^4.5.0",
    "axios": "^1.8.1",
    "classnames": "^2.5.1",
    "framer-motion": "^12.23.12",
    "fs-extra": "^11.3.0",
    "js-file-download": "^0.4.12",
    "lodash": "^4.17.21",
    "oidc-client-ts": "^3.1.0",
    "path": "^0.12.7",
    "pdfjs-dist": "2.6.347",
    "polished": "^4.3.1",
    "qs": "^6.14.0",
    "react": "^19.0.0",
    "react-apexcharts": "^1.7.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-big-calendar": "^1.18.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.0.0",
    "react-highlight-words": "^0.21.0",
    "react-image-file-resizer": "^0.4.8",
    "react-infinite-scroll-component": "^6.1.0",
    "react-oidc-context": "^3.2.0",
    "react-onesignal": "^3.0.1",
    "react-pdf": "^9.2.1",
    "react-responsive": "^10.0.0",
    "react-router": "^7.2.0",
    "react-split-pane": "^2.0.3",
    "react-to-print": "^3.0.5",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.15",
    "tinymce": "^7.7.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/history": "^5.0.0",
    "@types/lodash": "^4.17.16",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.0.10",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-big-calendar": "^1.16.1",
    "@types/react-dom": "^19.0.4",
    "@types/react-pdf": "^7.0.0",
    "eslint": "^9.21.0",
    "express": "^4.21.2",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.9",
    "stylelint": "^16.15.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": "22.x"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
